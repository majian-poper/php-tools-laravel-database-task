{
    "name": "php-tools/laravel-database-task",
    "keywords": [
        "laravel",
        "filamentphp"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "calebporzio/sushi": "^2.5",
        "composer/semver": "^3.4",
        "filament/filament": "^3.0 || ^4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "spatie/laravel-package-tools": "^1.16",
        "symfony/finder": "^7.4"
    },
    "require-dev": {
        "laravel/pint": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "PHPTools\\LaravelDatabaseTask\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PHPTools\\LaravelDatabaseTask\\DatabaseTaskServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
